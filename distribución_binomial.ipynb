      "source": [
        "def my_binomial(k, n, p):\r\n",
        "  return factorial(n)/(factorial(k)*factorial(n-k))*pow(p,k)*pow(1-p,n-k)"
      ],
      "execution_count": 1,
      "outputs": []
    },
